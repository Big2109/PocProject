{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Poc",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Poc",
            "program": "${workspaceFolder}/Poc/bin/Debug/net8.0/Poc.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Poc",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "console": "internalConsole"
        }
    ]
}